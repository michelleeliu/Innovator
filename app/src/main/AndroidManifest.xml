            </intent-filter>
        </activity>
        <activity android:name="com.innovator.solve.ReadingQuestions"></activity>
        <activity android:name="com.innovator.solve.MainMenuController"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.innovator.solve.DailyChallenge"
            android:exported="true">
