        </activity>
        <activity android:name="com.innovator.solve.SetupMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.innovator.solve.TopicSelectActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
